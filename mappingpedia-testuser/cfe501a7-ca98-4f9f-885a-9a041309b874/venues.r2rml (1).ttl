	rr:logicalTable [ 
        rr:tableName  "VENUES" # http://ma.tingtun.no/ogd/datasets/data/venues/?format=csv
    ];
	
    rr:subjectMap [    
		a rr:Subject; rr:termType rr:IRI; rr:class schema:Place;
